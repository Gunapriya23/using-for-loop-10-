import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter any number:" );
        int n=sc.nextInt();
        int mult;
        
        for(int i=1;i<=16;i=i+1){
            mult=i*n;
             System.out.println(i+"*"+n+"="+mult);
            
        }
       
    }
}
